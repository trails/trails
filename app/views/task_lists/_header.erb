<section class="<%= header.class.name.underscore %>"
 id="<%= header.class.name.underscore << "_" << header.id.to_s %>">
  <span class="external toolbar">
    <%= link_to "Delete", "#delete",:class=>"delete" %>
    <%= link_to "Edit", "#edit", :class=>"edit" %>
  </span>
  <span class="title">
    <%= header.title %>
  </span>
  <span class="new_task">
    <%= link_to "New task", new_task_list_task_url(header) %>
  </span>
</section>
<%= render :partial=>"task_lists/edit", :object=>header %>
